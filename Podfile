  target 'WorkShopSearcherTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'RxTest'
    pod 'Mockingjay', '3.0.0-alpha.1'
  end
end

target 'WorkShopSearcherUITests' do
  inherit! :search_paths
  # Pods for testing
end



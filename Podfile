# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'WorkShopSearcher' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WorkShopSearcher
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Alamofire'
  pod 'XLPagerTabStrip'
  pod 'RealmSwift'
  pod 'SkeletonView'
  pod 'SwiftMessages'
  pod 'SwiftLint'

  target 'WorkShopSearcherTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'RxTest'
    pod 'Mockingjay'
  end

  target 'WorkShopSearcherUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
